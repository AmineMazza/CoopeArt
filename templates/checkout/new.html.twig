                    <!--===============================================================================================-->	
              <script src="/assets/FrontOffice/vendor/jquery/jquery-3.2.1.min.js"></script>
            <!--===============================================================================================-->
              <script src="/assets/FrontOffice/vendor/animsition/js/animsition.min.js"></script>
            <!--===============================================================================================-->
              <script src="/assets/FrontOffice/vendor/bootstrap/js/popper.js"></script>
              <script src="/assets/FrontOffice/vendor/bootstrap/js/bootstrap.min.js"></script>
            <!--===============================================================================================-->
              <script src="/assets/FrontOffice/vendor/select2/select2.min.js"></script>
              <script>
                $(".js-select2").each(function(){
                  $(this).select2({
                    minimumResultsForSearch: 20,
                    dropdownParent: $(this).next('.dropDownSelect2')
                  });
                })
              </script>
            <!--===============================================================================================-->
              <script src="/assets/FrontOffice/vendor/daterangepicker/moment.min.js"></script>
              <script src="/assets/FrontOffice/vendor/daterangepicker/daterangepicker.js"></script>
            <!--===============================================================================================-->
              <script src="/assets/FrontOffice/vendor/slick/slick.min.js"></script>
